using System;
public class Maths 
{
    public int Add(int a , int b) 
    {
        return a + b;
    }
    public double Add(double a, double b) 
    {
        return a + b;
    }

    public int Add(int a, int b, int c) 
    {
        return a + b + c;
    }

    public string Add(string a,string b) 
    {
        return a + b ; 
    }
}

class Program 
{
    public static void Main() 
    {
        Maths calculate = new Maths();
       int sum = calculate.Add(1, 1);
        double sumdouble = calculate.Add(1.1, 3.4);
        int three = calculate.Add(10, 20, 30);
        string str = calculate.Add("Vivo","Book");

        Console.WriteLine($"sum of two num : {sum}");
        Console.WriteLine($"sum of double num : {sumdouble}");
        Console.WriteLine($"sum of three : {three}");
        Console.WriteLine($"sum of string : {str}");
    }
}

/*
 * sum of two num : 2
sum of double num : 4.5
sum of three : 60
sum of string : VivoBook
*/
